<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.990"
       name="990.ro"
       version="0.0.7"
       provider-name="krysty">
    <requires>
        <import addon="xbmc.python" version="2.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
		<provides>video</provides>
        <platform>all</platform>
        <summary>Stream videos from www.990.ro</summary>
        <description>Stream and download scene releases from the links listed on [COLOR blue]www.990.ro[/COLOR]</description>
    </extension>
</addon>

<addon id="plugin.video.antena3"
       name="Antena 3"
       version="1.0.0"
       provider-name="krysty">
  <requires>
   <import addon="xbmc.python" version="2.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Antena 3 RO live</summary>
    <description>Urmareste Antena 3 RO live.</description>
    <platform>all</platform>
	<language>ro</language>
  </extension>
</addon>

<addon id="repository.googlecode.krysty-xbmc" name="krysty-xbmc addons" version="1.0.0" provider-name="krysty">
	<extension point="xbmc.addon.repository" name="anarchintosh add-on repository">
		<info compressed="false">https://krysty-xbmc.googlecode.com/svn/addons/addons.xml</info>
		<checksum>https://krysty-xbmc.googlecode.com/svn/addons/addons.xml.md5</checksum>
		<datadir zip="true">https://krysty-xbmc.googlecode.com/svn/addons</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Install Addons from http://krysty-xbmc.googlecode.com</summary>
		<description>Download and install addons from krysty-xbmc repository.</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
